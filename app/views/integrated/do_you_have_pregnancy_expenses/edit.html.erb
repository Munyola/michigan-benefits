<% content_for :header_title, "Pregnancy" %>
<% content_for :form_card_title,
  "Do you have medical bills related to pregnancy from the last three months?" %>

<% content_for :form_card_body do %>
  <%= fields_for(:form, @form, builder: MbFormBuilder) do |f| %>
    <%= f.mb_yes_no_buttons :pregnancy_expenses,
                            yes_value: "yes",
                            no_value: "no" %>
  <% end %>
<% end %>

<% content_for :skip_footer, true %>
